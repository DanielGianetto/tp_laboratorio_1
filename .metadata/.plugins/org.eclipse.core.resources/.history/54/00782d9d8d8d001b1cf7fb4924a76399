#include "utn_ope.h"

float suma(float x, float y, float* pResultado)
{
	*pResultado = x+y;

	return 0;
}

float resta(float x, float y, float* pResultado)
{
	*pResultado = x+y;

	return 0;
}

float multiplicacion(float x, float y, float* pResultado)
{
	*pResultado = x*y;

	return 0;
}

float division(float x, float y, float* pResultado)
{
	*pResultado = x/y;

	return 0;
}

long long factorial(float x)
{
	int fact;
	int i;

	fact = 1;

	if(x == 0)
	{
		fact = 1;
	}
	else
	{
		for(i = 1; i <= x; i++)
		{
			fact = fact * i;
		}
	}
}
