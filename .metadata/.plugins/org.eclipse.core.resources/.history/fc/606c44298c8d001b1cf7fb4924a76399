#include "utn_ope.h"

float suma(float x, float y, float* pResultado)
{
	*pResultado = x+y;

	return 0;
}

float resta(float x, float y, float* pResultado)
{
	*pResultado = x+y;

	return 0;
}

float multiplicacion(float x, float y, float* pResultado)
{
	*pResultado = x*y;

	return 0;
}
